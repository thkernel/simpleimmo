<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Building</th>
      <th>Landlord</th>
      <th>Property type</th>
      <th>Reference</th>
      <th>Description</th>
      <th>Pieces number</th>
      <th>Room number</th>
      <th>Bath room</th>
      <th>Rent excluding tax</th>
      <th>Address</th>
      <th>City</th>
      <th>Neighborhood</th>
      <th>Street</th>
      <th>Door</th>
      <th>Surface</th>
      <th>Value</th>
      <th>About</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.building %></td>
        <td><%= property.landlord %></td>
        <td><%= property.property_type %></td>
        <td><%= property.reference %></td>
        <td><%= property.description %></td>
        <td><%= property.pieces_number %></td>
        <td><%= property.room_number %></td>
        <td><%= property.bath_room %></td>
        <td><%= property.rent_excluding_tax %></td>
        <td><%= property.address %></td>
        <td><%= property.city %></td>
        <td><%= property.neighborhood %></td>
        <td><%= property.street %></td>
        <td><%= property.door %></td>
        <td><%= property.surface %></td>
        <td><%= property.value %></td>
        <td><%= property.about %></td>
        <td><%= property.status %></td>
        <td><%= property.user %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
