<p id="notice"><%= notice %></p>

<h1>Provider Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @provider_types.each do |provider_type| %>
      <tr>
        <td><%= provider_type.name %></td>
        <td><%= provider_type.description %></td>
        <td><%= provider_type.status %></td>
        <td><%= provider_type.user %></td>
        <td><%= link_to 'Show', provider_type %></td>
        <td><%= link_to 'Edit', edit_provider_type_path(provider_type) %></td>
        <td><%= link_to 'Destroy', provider_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Provider Type', new_provider_type_path %>
