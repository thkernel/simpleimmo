<% @store_types.each do |store_type| %>
      <tr>
        <td><%= store_type.name %></td>
        <td><%= store_type.description %></td>
       
        <td>
            <div class="action-buttons">
                    <%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'), edit_store_type_path(store_type), title: "Modifier", remote: true %>




<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), store_type_delete_path(store_type), remote: true, title: "Supprimer", class: "delete" %>
</div>
        </td>
      </tr>
    <% end %>