<p id="notice"><%= notice %></p>

<h1>Landlords</h1>

<table>
  <thead>
    <tr>
      <th>Landlord type</th>
      <th>Civility</th>
      <th>Last name</th>
      <th>First name</th>
      <th>Birth date</th>
      <th>Birth place</th>
      <th>Id type</th>
      <th>Id number</th>
      <th>Id expiration</th>
      <th>Profession</th>
      <th>Nationality</th>
      <th>Company name</th>
      <th>Address</th>
      <th>Phone</th>
      <th>City</th>
      <th>Country</th>
      <th>Email</th>
      <th>About</th>
      <th>User</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @landlords.each do |landlord| %>
      <tr>
        <td><%= landlord.landlord_type %></td>
        <td><%= landlord.civility %></td>
        <td><%= landlord.last_name %></td>
        <td><%= landlord.first_name %></td>
        <td><%= landlord.birth_date %></td>
        <td><%= landlord.birth_place %></td>
        <td><%= landlord.id_type %></td>
        <td><%= landlord.id_number %></td>
        <td><%= landlord.id_expiration %></td>
        <td><%= landlord.profession %></td>
        <td><%= landlord.nationality %></td>
        <td><%= landlord.company_name %></td>
        <td><%= landlord.address %></td>
        <td><%= landlord.phone %></td>
        <td><%= landlord.city %></td>
        <td><%= landlord.country %></td>
        <td><%= landlord.email %></td>
        <td><%= landlord.about %></td>
        <td><%= landlord.user %></td>
        <td><%= landlord.status %></td>
        <td><%= link_to 'Show', landlord %></td>
        <td><%= link_to 'Edit', edit_landlord_path(landlord) %></td>
        <td><%= link_to 'Destroy', landlord, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Landlord', new_landlord_path %>
