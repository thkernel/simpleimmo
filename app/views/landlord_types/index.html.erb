<p id="notice"><%= notice %></p>

<h1>Landlord Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @landlord_types.each do |landlord_type| %>
      <tr>
        <td><%= landlord_type.name %></td>
        <td><%= landlord_type.description %></td>
        <td><%= landlord_type.status %></td>
        <td><%= landlord_type.user %></td>
        <td><%= link_to 'Show', landlord_type %></td>
        <td><%= link_to 'Edit', edit_landlord_type_path(landlord_type) %></td>
        <td><%= link_to 'Destroy', landlord_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Landlord Type', new_landlord_type_path %>
