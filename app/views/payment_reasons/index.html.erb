<p id="notice"><%= notice %></p>

<h1>Payment Reasons</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @payment_reasons.each do |payment_reason| %>
      <tr>
        <td><%= payment_reason.name %></td>
        <td><%= payment_reason.description %></td>
        <td><%= payment_reason.status %></td>
        <td><%= payment_reason.user %></td>
        <td><%= link_to 'Show', payment_reason %></td>
        <td><%= link_to 'Edit', edit_payment_reason_path(payment_reason) %></td>
        <td><%= link_to 'Destroy', payment_reason, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Payment Reason', new_payment_reason_path %>
