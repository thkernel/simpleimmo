<p id="notice"><%= notice %></p>

<h1>Leases</h1>

<table>
  <thead>
    <tr>
      <th>Tenant</th>
      <th>Building</th>
      <th>Property</th>
      <th>Usage type</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Effective date</th>
      <th>Duration</th>
      <th>Id type</th>
      <th>Id number</th>
      <th>Periodicity</th>
      <th>Payment method</th>
      <th>Payment date</th>
      <th>Receipt date</th>
      <th>Rent excluding tax</th>
      <th>Vat rate</th>
      <th>Net amount</th>
      <th>Vat paid by tenant</th>
      <th>Guarantee amount</th>
      <th>Avance</th>
      <th>Renewable</th>
      <th>Status</th>
      <th>Notes</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leases.each do |lease| %>
      <tr>
        <td><%= lease.tenant %></td>
        <td><%= lease.building_id %></td>
        <td><%= lease.property_id %></td>
        <td><%= lease.usage_type %></td>
        <td><%= lease.start_date %></td>
        <td><%= lease.end_date %></td>
        <td><%= lease.effective_date %></td>
        <td><%= lease.duration %></td>
        <td><%= lease.id_type %></td>
        <td><%= lease.id_number %></td>
        <td><%= lease.periodicity %></td>
        <td><%= lease.payment_method %></td>
        <td><%= lease.payment_date %></td>
        <td><%= lease.receipt_date %></td>
        <td><%= lease.rent_excluding_tax %></td>
        <td><%= lease.vat_rate %></td>
        <td><%= lease.net_amount %></td>
        <td><%= lease.vat_paid_by_tenant %></td>
        <td><%= lease.guarantee_amount %></td>
        <td><%= lease.avance %></td>
        <td><%= lease.renewable %></td>
        <td><%= lease.status %></td>
        <td><%= lease.notes %></td>
        <td><%= lease.user %></td>
        <td><%= link_to 'Show', lease %></td>
        <td><%= link_to 'Edit', edit_lease_path(lease) %></td>
        <td><%= link_to 'Destroy', lease, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lease', new_lease_path %>
