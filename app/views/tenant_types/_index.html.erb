<% @tenant_types.each do |tenant_type| %>
  <tr>
    <td><%= tenant_type.name %></td>
    
    <td>
            <div class="action-buttons">
                    <%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'), edit_tenant_type_path(tenant_type), title: "Modifier", remote: true %>




<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), tenant_type_delete_path(tenant_type), remote: true, title: "Supprimer", class: "delete" %>
</div>
        </td>
  </tr>
<% end %>