<% @movement_types.each do |movement_type| %>
      <tr>
        <td><%= movement_type.name %></td>
        <td><%= movement_type.description %></td>
       
        
        <td>
					<div class="action-buttons">
					<%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'), edit_movement_type_path(movement_type), title: "Modifier", remote: true %>




<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), movement_type_delete_path(movement_type), remote: true, title: "Supprimer", class: "delete" %>
</div>
</td>
      </tr>
    <% end %>